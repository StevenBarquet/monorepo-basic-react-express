{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "predev": "cross-env NODE_ENV=development",
    "prebuild": "cross-env NODE_ENV=production node ./config/git-hooks/scripts/pre-run.js",
    "dev": "nodemon -e ts --ignore dist/  --ignore src/entities/health/config/ --watch src/ --exec 'tsc && tsc-alias && node ./dist/backend/src/index.js'",
    "prod": "npm run build && cross-env NODE_ENV=production DEBUG=app:prod node ./dist/backend/src/index.js",
    "build": "tsc && tsc-alias",
    "start": "node ./dist/backend/src/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "7.0.3",
    "debug": "4.3.4",
    "express": "4.21.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/debug": "4.1.12",
    "@types/express": "5.0.0",
    "@types/node": "22.9.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-prettier": "4.0.0",
    "nodemon": "2.0.16",
    "tsc-alias": "1.8.10",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": "20.x"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development",
      "DEBUG": "app:*"
    }
  }
}
